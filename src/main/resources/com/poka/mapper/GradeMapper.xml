<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.poka.mapper.GradeMapper">

	<insert id="insert">
		<selectKey keyProperty="grade_code" order="BEFORE" resultType="String">
			SELECT 'A'||seq_grade.NEXTVAL FROM dual
		</selectKey>
		insert into p_grade(grade_code, grade_name, grade_img, grade_lv)
		values(#{grade_code}, #{grade_name}, #{grade_img}, #{grade_lv})
    </insert>
    
    <update id="update">
    
    </update>
    
    <delete id="delete">
    
    </delete>
    
    <select id="read" resultType="com.poka.domain.GradeVO">
    </select>
    
    <select id="getList" 
	        resultType="com.poka.domain.GradeVO">
	        select *  from p_grade where grade_code IS NOT NULL
	</select>
</mapper>







